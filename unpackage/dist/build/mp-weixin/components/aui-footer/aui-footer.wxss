.aui-footer{width:100vw;height:55px;background:#fff;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-justify-content:space-around;justify-content:space-around;white-space:nowrap;position:fixed!important;bottom:0;left:0;z-index:997}.aui-footer-list{padding:7px 20px 3px 20px;max-width:25%;text-align:center;-webkit-box-flex:1;-webkit-flex:1;flex:1;-webkit-box-sizing:border-box;box-sizing:border-box;display:inline-block}.aui-footer-list .iconfont{width:100%;height:25px;line-height:28px;display:inline-block;font-size:22px;background:-webkit-gradient(linear,left top,right top,from(#aaa),to(#cdcdcd));background:-webkit-linear-gradient(left,#aaa,#cdcdcd);background:linear-gradient(90deg,#aaa,#cdcdcd);-webkit-background-clip:text;color:transparent}.aui-footer-list .aui-footer-text{width:100%;height:20px;line-height:20px;font-size:12px;background:-webkit-gradient(linear,left top,right top,from(#aaa),to(#cdcdcd));background:-webkit-linear-gradient(left,#aaa,#cdcdcd);background:linear-gradient(90deg,#aaa,#cdcdcd);-webkit-background-clip:text;color:transparent}.aui-footer-list.active .iconfont,.aui-footer-list.active .aui-footer-text{background:-webkit-gradient(linear,left top,right top,from(#197de0),to(#73b9ec));background:-webkit-linear-gradient(left,#197de0,#73b9ec);background:linear-gradient(90deg,#197de0,#73b9ec);-webkit-background-clip:text;color:transparent}